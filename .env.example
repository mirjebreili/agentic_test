# --- Execution Mode ---
MODE=PAPER                   # BACKTEST | PAPER | LIVE

# --- LLM Configuration (OpenAI-compatible) ---
# This can point to VLLM, Ollama's OpenAI-compat endpoint, or any other compliant server.
# Should be the ROOT of the server, e.g., http://localhost:11434 (the app will add /v1)
OPENAI_BASE_URL=http://localhost:8000
OPENAI_MODEL=Qwen/Qwen2.5-7B-Instruct
OPENAI_API_KEY=not-needed-for-local

# --- Broker/Data ---
DATA_CACHE_FORMAT=csv       # csv | parquet | none
# OANDA keys are only needed if broker_provider = "oanda"
OANDA_ENV=practice          # practice | live
OANDA_API_KEY=
OANDA_ACCOUNT_ID=

# --- Optional Providers ---
ALPHA_VANTAGE_KEY=
FINNHUB_API_KEY=
TWELVEDATA_KEY=
TE_KEY=                     # Trading Economics
RUN_TRADING=true

# --- Telemetry (Optional) ---
# Set to true to enable LangSmith cloud tracing
LANGSMITH_TRACING=false
LANGSMITH_API_KEY=
LANGSMITH_PROJECT=
# LANGSMITH_ENDPOINT=https://api.smith.langchain.com
